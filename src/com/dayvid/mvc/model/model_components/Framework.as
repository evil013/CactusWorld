package com.dayvid.mvc.model.model_components {	import com.dayvid.mvc.composite_view.CompositeRootView;		import flash.display.DisplayObjectContainer;		import mvc_framework.controller.Controller;
	public class Framework extends LocalSharedObjectData 	{		private var _controller:Controller;		private var _rootView:CompositeRootView;				public function Framework() 		{			super();		}				public function set controller(aParam:Controller):void 		{			_controller = aParam;		}		public function get controller():Controller		{			return _controller;		}				public function set rootView(aParam:CompositeRootView):void 		{			_rootView = aParam;		}		public function get rootView():CompositeRootView		{			return _rootView;		}				public function set targetStage_mc(aParam:DisplayObjectContainer):void 		{			_targetStage_mc = aParam;		}		public function get targetStage_mc():DisplayObjectContainer		{			return _targetStage_mc;		}	}}