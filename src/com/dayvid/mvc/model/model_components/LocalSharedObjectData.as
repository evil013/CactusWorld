package com.dayvid.mvc.model.model_components {	import flash.net.SharedObject;		public class LocalSharedObjectData extends OptionsSettings 	{		private var _localSharedObject:SharedObject;				public function LocalSharedObjectData()		{			super();		}				public function initLocalSharedObject(name:String):SharedObject		{			try			{				_localSharedObject = SharedObject.getLocal(name);			}			catch (e:Error)			{				_localSharedObject = null;			}						return _localSharedObject;		}				public function get localSharedObject():SharedObject		{			return _localSharedObject;		}				public function flushLocalSharedObject():void		{			try			{				if (_localSharedObject != null) 				{					_localSharedObject.flush();				}			}			catch (e:Error)			{							}		}	}}