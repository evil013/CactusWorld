package mvc_framework.sound.musicfactory{	import com.dayvid.mvc.model.model_components.OptionsSettings;	import com.dayvid.mvc.sound.music.MusicCreator;		import mvc_framework.sound.MusicList;
	// MusicCreator encapsulates music creation	public class MusicProjectCreator extends MusicCreator	{		private var embedSound:Object;				public function MusicProjectCreator(aModel:OptionsSettings, aEmbedSound:Object) 		{			super(aModel);						embedSound = aEmbedSound;		}				override protected function createSound (aType:uint):Class 		{			if (aType == MusicList.MENU_LOOP) 			{				return embedSound.MenuLoop;			} 			else if (aType == MusicList.INTRO_THEME_LOOP) 			{				return embedSound.IntroThemeLoop;			} 			else if (aType == MusicList.SHOP_TIME_LOOP) 			{				return embedSound.ShopTimeLoop;			} 			else if (aType == MusicList.SHOPPING_LOOP) 			{				return embedSound.ShoppingLoop;			}			else if (aType == MusicList.GAME_OVER) 			{				return embedSound.GameOver;			}			else if (aType == MusicList.RACE_REGION_LOOP_EXTERNAL) 			{				_playDelete = true;				return embedSound.TrackMusicClass;			} 			else 			{				return super.createSound(aType);			}		}	}}