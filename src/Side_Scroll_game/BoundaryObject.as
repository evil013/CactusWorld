//Abstract class for player and enemypackage  {	import flash.display.MovieClip;	import flash.events.Event;		public class BoundaryObject extends MovieClip {		private var downwardVelocity:Number;//物體下降速度		protected var isRunning:Boolean;//判斷物體是否有在動作		//init()		public function BoundaryObject() {						trace ("i am any object that collides with the boundary" );						downwardVelocity = 0;						isRunning = false;						this.gotoAndStop("jump");						addEventListener(Event.ENTER_FRAME, enterFrameHandler );		}		//enterFrame function		private function enterFrameHandler (event:Event ) :void {						downwardVelocity += 2; // equals itself plus 2			this.y += downwardVelocity;//讓物體下降					}				public function incrementUpward() 		{			//increment the y up until not colliding			this.y -= 1;					}		public function keepOnBoundary() 		{			downwardVelocity = 0; // stops pulling the object down						positionOnLand();					}		//物件動作模式		public function positionOnLand ()		{						//let this be overridden by the subclass					}	}}